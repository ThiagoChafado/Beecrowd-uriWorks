import java.util.Scanner;

public class Main {

    
    public static void main(String args[]) {
       Scanner sc = new Scanner(System.in);
       int V[] = new int[10];
       int n = sc.nextInt();
       V[0] = n;
       for(int i =1;i<10;i++){
           V[i] = V[i-1] *2;
       }
       for(int i = 0;i<10;i++){
           System.out.printf("N[%d] = %d\n",i,V[i]);
       }
    }
}