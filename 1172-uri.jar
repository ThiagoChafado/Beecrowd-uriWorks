import java.util.Scanner;

public class Main {

    
    public static void main(String args[]) {
       Scanner sc = new Scanner(System.in);
       int V[] = new int[10];
       for(int i = 0;i<10;i++){
           V[i] = sc.nextInt();
           if (V[i] <= 0){
               V[i] = 1;
           }
       }for(int i=0;i<10;i++){
            
            System.out.printf("X[%d] = %d\n",i,V[i]);
       }
    }
}

